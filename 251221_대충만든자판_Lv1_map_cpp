//O(keymap + targets)
//map기억하기
#include <string>
#include <vector>
#include <map>

using namespace std;

vector<int> solution(vector<string> keymap, vector<string> targets) {
    vector<int> answer;
    map<int,int> maps;
    for (int i=0;i<keymap.size();i++){
        for(int j=0;j<keymap[i].size();j++){
            if (maps[keymap[i][j]] == 0) maps[keymap[i][j]] = j+1;
            else maps[keymap[i][j]] = min(maps[keymap[i][j]],j+1);
        }
    }
    for (int i=0;i<targets.size();i++){
        answer.push_back(0);
        for(int j=0;j<targets[i].size();j++){
            if(maps[targets[i][j]] == 0){
                answer[answer.size()-1]  = -1 ;
                break;
            }
            answer[answer.size()-1] += maps[targets[i][j]];
        }
    }
    
    return answer;
}
